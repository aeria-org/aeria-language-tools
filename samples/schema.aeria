collection Person {
  required {
    name
  }

  properties {
    name str
    age int @maximum(10)
    picture File @accept(["image/*"])
  }

  functions {
    get
    getAll
    custom? @expose('unauthenticated')
  }
}

collection Pet {
  properties {
    name str
    specie enum @options([
      "dog",
      "cat"
    ])
  }
}

