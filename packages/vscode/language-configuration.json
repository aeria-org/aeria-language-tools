{
	"comments": {
		"lineComment": "--"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		{
			"open": "\\"",
			"close": "\\"",
			"notIn": [
				"string"
			]
		}
	],
	"surroundingPairs": [
			["{", "}"],
			["[", "]"],
			["(", ")"],
			["\\"", "\\""]
	],
	"folding": {
		"offSide": true,
		"markers": {
			"start": "^\\s*#region\\b",
			"wnd": "^\\s*#endregion\\b"
		}
	}
}
